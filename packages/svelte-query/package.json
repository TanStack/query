{
  "name": "@tanstack/svelte-query",
  "version": "4.12.0",
  "description": "Primitives for managing, caching and syncing asynchronous and remote data in Solid",
  "author": "Dre Johnson",
  "license": "MIT",
  "repository": "tanstack/query",
  "homepage": "https://tanstack.com/query",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/tannerlinsley"
  },
  "types": "build/svelte/index.d.ts",
  "main": "build/lib/packages/svelte-query/src/index.js",
  "module": "build/lib/packages/svelte-query/src/index.esm.js",
  "exports": {
    ".": {
      "types": "./build/svelte/index.d.ts",
      "import": "./build/lib/packages/svelte-query/src/index.mjs",
      "browser": "./build/lib/packages/svelte-query/src/index.mjs",
      "require": "./build/lib/packages/svelte-query/src/index.js",
      "node": "./build/lib/packages/svelte-query/src/index.js",
      "default": "./build/lib/packages/svelte-query/src/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf ./build",
    "test:eslint": "../../node_modules/.bin/eslint --ext .ts,.tsx ./src",
    "test:jest": "../../node_modules/.bin/jest --config ./jest.config.ts",
    "test:dev": "pnpm run test:jest --watch"
  },
  "files": [
    "build/lib/*",
    "build/umd/*",
    "build/svelte/*",
    "src"
  ],
  "devDependencies": {
    "svelte": "^3.52.0"
  },
  "dependencies": {
    "@tanstack/query-core": "workspace:*"
  },
  "peerDependencies": {
    "svelte": "^3.52.0"
  },
  "peerDependenciesMeta": {}
}
