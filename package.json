    "@babel/preset-react": "^7.0.0",
    "@svgr/rollup": "^4.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.x",
    "babel-jest": "^24.9.0",
    "cross-env": "^5.1.4",
    "eslint": "5.x",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-react-app": "^4.0.1",
