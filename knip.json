{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "ignore": [
    ".pnpmfile.cjs",
    "scripts/*.js",
    "**/root.*.config.*",
    "**/ts-fixture/file.ts"
  ],
  "ignoreDependencies": [
    "@types/react",
    "@types/react-dom",
    "react",
    "react-dom"
  ],
  "ignoreWorkspaces": ["examples/**", "integrations/**"],
  "workspaces": {
    "packages/angular-query-experimental": {
      "ignore": ["**/production/index.ts"]
    },
    "packages/query-codemods": {
      "entry": ["src/v4/**/*.cjs", "src/v5/**/*.cjs"],
      "ignore": ["**/__testfixtures__/**"]
    },
    "packages/vue-query": {
      "ignore": ["**/__mocks__/**"],
      "ignoreDependencies": ["vue2", "vue2.7"]
    }
  }
}
